{
	"name": "esm-env",
	"version": "1.2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/benmccann/esm-env.git"
	},
	"license": "MIT",
	"homepage": "https://github.com/benmccann/esm-env",
	"author": "Ben McCann (https://www.benmccann.com)",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./index.js"
		},
